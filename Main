local Tab = Window:CreateTab("Main")

local Section = Tab:CreateSection("Locations:")


local Dropdown = Tab:CreateDropdown({
    Name = "Select:",
    Options = {"Spawn","Volcano","Ghost Mansion","Cave","Pyramid"},
    CurrentOption = {"None"},
    MultipleOptions = false,
    Flag = "Dropdown1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Option)

    

     if Option[1] == "Spawn" then
        
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1271, 98, -1165)


    elseif Option[1] == "Volcano" then

        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(208, 252, -1)



      elseif Option[1] == "Ghost Mansion" then

         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1363, 164, 160)


      elseif Option[1] == "Cave" then

         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1905, 120, -627)

      elseif Option[1] == "Pyramid" then
         
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2285, 199, 300)


      end
   
   
    end,
 })



getgenv() AutoFarm = true

local Section = Tab:CreateSection("Afk Farming:")
local Toggle = Tab:CreateToggle({
    Name = "Activate",
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
   
    AutoFarm = Value
    
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(183, 290, -107)
    task.wait(1)

    while AutoFarm == true do
        
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2569, 162, 4620)
    task.wait()


    if AutoFarm == false then

    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2608, 162, 4635)

    end

    end

    end,
 })

